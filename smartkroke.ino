float curX = 0;
float curY = 0;
float curZ = 0;
float speedX = 0;
float speedY = 0;
float speedZ = 0;
long t0 = millis();
#define sign(x) ((x) < 0 ? -1 : ((x) > 0 ? 1 : 0))

void updateTranslation(){
  IMU.readAcceleration(ax, ay, az);
  IMU.readGyroscope(gx, gy, gz);
  IMU.readMagneticField(mx, my, mz);
  deltat = fusion.deltatUpdate();
  fusion.MahonyUpdate(-gy*DEG_TO_RAD, gz*DEG_TO_RAD, gx*DEG_TO_RAD,-ay*G,  az*G, ax*G, deltat);
  //fusion.MahonyUpdate(-gy*DEG_TO_RAD, gz*DEG_TO_RAD, gx*DEG_TO_RAD,-ay*G,  az*G, ax*G, -my, mz, -mx, deltat);
  //fusion.MadgwickUpdate(-gy*DEG_TO_RAD, gz*DEG_TO_RAD, gx*DEG_TO_RAD,-ay*G,  az*G, ax*G deltat);
  float* q = fusion.getQuat();

  roll = fusion.getRoll();
  pitch = fusion.getPitch();
  yaw = fusion.getYaw();// - yawCalibration;
  //Serial.println(fusion.getYaw());

// calculating this purely through quaternions would probably be more efficient if it comes to that, no idea how fast inverting is
  BLA::Matrix<3,3> rotationMatrix = {
    1- 2*q[2]*q[2] - 2*q[3]*q[3], 2*q[1]*q[2] - 2*q[0]*q[3]    , 2*q[1]*q[3] + 2*q[0]*q[2],
    2*q[1]*q[2] + 2*q[0]*q[3]    ,  1 - 2*q[1]*q[1] - 2*q[3]*q[3], 2*q[2]*q[3] - 2*q[0]*q[1],
    2*q[1]*q[3] - 2*q[0]*q[2]    , 2*q[2]*q[3] + 2*q[0]*q[1]    , 1 - 2*q[1]*q[1] - 2*q[2]*q[2]
  };
  Invert(rotationMatrix);
  
  BLA::Matrix<3,1> Acc = {-ay,az,ax};
  BLA::Matrix<3,1> G = {0,0,1};

  Acc = Acc - rotationMatrix*G;

//effectively simples possible kalman filter
  float dt = (millis()-t0)/1000.0;
  t0 = millis();
  speedX *= 0.9;
  speedY *= 0.9;
  speedZ *= 0.9;

  
  curX *= 0.9;
  curY *= 0.9;
  curZ *= 0.9;
  speedX += dt * Acc(0)*9.81;
  speedY += dt * Acc(1)*9.81;
  speedZ += dt * Acc(2)*9.81;
  curX += speedX * dt;
  curY += speedY * dt;
  curZ += speedZ * dt;

  
  Serial.print(curX*100);
  Serial.println();
}
  
  




